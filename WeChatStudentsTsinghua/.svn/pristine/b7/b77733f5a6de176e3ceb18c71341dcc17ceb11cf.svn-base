package students.tsinghua.pojo;

/**
 * Pojo TShow -- TShow table
 * 
 * @author hujiawei
 * 
 */
public class TShow {

	private int id;
	private int showId;// show id -- in related to "http://www.hall.tsinghua.edu.cn/ycshow.aspx?id=235"
	private String name;
	private String location;//
	private String date;// date -- 2013-5-17
	private String time;// time -- 9:30
	private String team;//
	private String host;// host organization
	private String price;
	private String ticketstate;// the state of the price selling
	private String desc;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getShowId() {
		return showId;
	}

	public void setShowId(int showId) {
		this.showId = showId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getLocation() {
		return location;
	}

	public void setLocation(String location) {
		this.location = location;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public String getTeam() {
		return team;
	}

	public void setTeam(String team) {
		this.team = team;
	}

	public String getHost() {
		return host;
	}

	public void setHost(String host) {
		this.host = host;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getTicketstate() {
		return ticketstate;
	}

	public void setTicketstate(String ticketstate) {
		this.ticketstate = ticketstate;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

}
